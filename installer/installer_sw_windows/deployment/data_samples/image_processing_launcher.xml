<?xml version="1.0" encoding="ISO-8859-1"?>
<launcher xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="launcher.xsd">
  <id>IMAGE_PROCESSING_LAUNCHER</id>
  <description>Example launcher for the image processing workflow</description>
  <workflow_id>IMAGE_PROCESSING_WORKFLOW</workflow_id>
  <parameters>
    <parameter>
      <id>RADIUS</id>
      <value>6</value>
      <task_id>BLUR</task_id>
      <task_sequential_id>1</task_sequential_id>
    </parameter>
    <parameter>
      <id>SIGMA</id>
      <value>3</value>
      <task_id>BLUR</task_id>
      <task_sequential_id>1</task_sequential_id>
    </parameter>
    <parameter>
      <id>RADIUS</id>
      <value>5</value>
      <task_id>CHARCOAL</task_id>
      <task_sequential_id>2</task_sequential_id>
    </parameter>
    <parameter>
      <id>SIGMA</id>
      <value>2</value>
      <task_id>CHARCOAL</task_id>
      <task_sequential_id>2</task_sequential_id>
    </parameter>
    <parameter>
      <id>RADIUS</id>
      <value>6</value>
      <task_id>OILPAINT</task_id>
      <task_sequential_id>3</task_sequential_id>
    </parameter>
    <parameter>
      <id>FLIP_MODE</id>
      <value>0</value>
      <task_id>MIRROR</task_id>
      <task_sequential_id>7</task_sequential_id>
    </parameter>
    <parameter>
      <id>DEGREES</id>
      <value>90</value>
      <task_id>ROTATE</task_id>
      <task_sequential_id>8</task_sequential_id>
    </parameter>
  </parameters>
  <repositories>
    <repository>
      <id>REPO_1</id>
      <numerical_id>0</numerical_id>
      <path>C:/data/input</path>
    </repository>
    <repository>
      <id>REPO_2</id>
      <numerical_id>1</numerical_id>
      <path>C:/data/output</path>
    </repository>
  </repositories>
  <connections>
    <connection>
      <from>
        <type>repository</type>
        <numerical_id>0</numerical_id>
        <slot>0</slot>
      </from>
      <to>
        <type>task</type>
        <numerical_id>12</numerical_id>
        <slot>0</slot>
      </to>
      <file_name>airplane.png</file_name>
    </connection>
    <connection>
      <from>
        <type>task</type>
        <numerical_id>2</numerical_id>
        <slot>0</slot>
      </from>
      <to>
        <type>repository</type>
        <numerical_id>1</numerical_id>
        <slot>0</slot>
      </to>
      <file_name>result.png</file_name>
    </connection>
  </connections>
</launcher>
